#include <stdio.h>

int main() {
    int A[] = {12, 21, 16, 19, 18, 16, 22, 20, 17, 15};
    int oddCount = 0, evenCount = 0;

    for (int i = 0; i < 10; i++) {
        if (A[i] % 2 == 0) {
            evenCount++;
        } else {
            oddCount++;
        }
    }

    printf("Odd = %d  Even = %d\n", oddCount, evenCount);

    return 0;
}
